{
  "name": "svelte-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm run typeshare && rollup -c",
    "check": "npm run typeshare && svelte-check --tsconfig ./tsconfig.json",
    "dev": "npm run typeshare && rollup -c -w",
    "lint": "npm run typeshare && eslint . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "pre-commit": "npm run lint:fix && npm run prettier",
    "prepare": "cd .. && husky install",
    "prettier": "prettier --write --plugin prettier-plugin-svelte .",
    "start": "npm run typeshare && sirv public --no-clear",
    "typeshare": "typeshare --lang=typescript --output-file=./src/typeshare.temp ../rust/src/ && cat ./src/missed.ts ./src/typeshare.temp > ./src/typeshare.ts"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.5.0",
    "@tsconfig/svelte": "^2.0.0",
    "@types/color-diff": "^1.2.2",
    "@types/lodash": "^4.14.199",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@wasm-tool/rollup-plugin-rust": "^2.4.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "rollup": "^2.79.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.6",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.59.2",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "color-diff": "^1.4.0",
    "konva": "^9.2.1",
    "lodash": "^4.17.21",
    "logrocket": "^5.0.1",
    "pretty-bytes": "^6.1.1",
    "rgb-hex": "^4.1.0",
    "sirv-cli": "^2.0.0",
    "svelte-konva": "^0.3.1",
    "sveltestrap": "^5.11.2"
  }
}
